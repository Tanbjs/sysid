{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Fast Run (no debug)",
        "type": "python",
        "request": "launch",
        "program": "${file}",
        "console": "integratedTerminal",
        "noDebug": false,
        "env": {
          "PYTHONPATH": "${workspaceFolder}"
        }
      }
    ]
  }

